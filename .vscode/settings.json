{
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "editor.formatOnSave": true,
    "files.watcherExclude": {
        "${workspaceFolder}/target/**": true
    },
    "python.analysis.autoSearchPaths": false,
    "python.analysis.extraPaths": [
        "${workspaceFolder}/src/main/python"
    ],
    "python.formatting.provider": "black",
    "python.linting.enabled": true,
    "python.sortImports.args": [
        "--profile",
        "black"
    ],
    "python.testing.cwd": "${workspaceFolder}/src/main/python",
    "python.testing.unittestArgs": [
        "-v",
        "-s",
        "${workspaceFolder}/src/unittest/python",
        "-p",
        "test_*.py"
    ],
    "python.testing.pytestEnabled": false,
    "python.testing.unittestEnabled": true,
    "qtForPython.designer.path": ".venv/Scripts/pyside6-designer.exe",
    "qtForPython.rcc.path": ".venv/Scripts/pyside6-rcc.exe",
    "qtForPython.uic.path": ".venv/Scripts/pyside6-uic.exe"
}